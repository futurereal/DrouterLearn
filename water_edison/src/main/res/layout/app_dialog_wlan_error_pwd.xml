<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="612dp"
    android:layout_height="373dp"
    android:background="@drawable/bg_dialog_single">

  <TextView
      android:id="@+id/filtererror_filter_type"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="35dp"
      android:text="@string/input_tips"
      android:textColor="@color/color_white"
      android:textSize="35dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/wifi_tips"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="8dp"
      android:text="--"
      android:textColor="@color/color_99"
      android:textSize="35dp"
      android:layout_marginLeft="40dp"
      android:layout_marginRight="40dp"
      app:layout_constraintTop_toBottomOf="@id/filtererror_filter_type"
      app:layout_constraintBottom_toTopOf="@id/sure_btn"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"/>

  <Button
      android:id="@+id/sure_btn"
      android:layout_width="256dp"
      android:layout_height="79dp"
      android:layout_marginBottom="35dp"
      android:background="@drawable/selector_wifi_finish"
      android:text="@string/sure"
      android:textColor="@color/color_white"
      android:textSize="28dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>